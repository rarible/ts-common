{
  "name": "@rarible/utils",
  "version": "0.10.0",
  "keywords": [
    "rarible",
    "common",
    "types"
  ],
  "homepage": "https://github.com/rarible/ts-common",
  "repository": {
    "type": "git",
    "url": "https://github.com/rariblecom/ts-common.git"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": {
        "require": "./build/index.d.ts",
        "import": "./esm/index.d.ts"
      },
      "require": "./build/index.js",
      "import": "./esm/index.js",
      "default": "./build/index.js"
    },
    "./build/": "./build/",
    "./esm/": "./esm/",
    "./*": {
      "types": {
        "require": "./build/*.d.ts",
        "import": "./esm/*.d.ts"
      },
      "require": "./build/*.js",
      "import": "./esm/*.js",
      "default": "./build/*.js"
    }
  },
  "main": "build/index.js",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "files": [
    "build",
    "esm"
  ],
  "scripts": {
    "build": "run-s build:*",
    "build:cjs": "tsc -p tsconfig.cjs.json --outDir ./build && printf '{\"type\":\"commonjs\"}' > ./build/package.json",
    "build:esm": "tsc -p tsconfig.esm.json --outDir ./esm && printf '{\"type\":\"module\"}' > ./esm/package.json",
    "clean": "rimraf ./build ./esm ./tsconfig.tsbuildinfo",
    "test": "jest --silent",
    "verify": "tsc"
  },
  "dependencies": {
    "bignumber.js": "^9.0.0",
    "generic-type-guard": "^3.7.2",
    "ts-custom-error": "^3.3.1"
  },
  "peerDependencies": {
    "tslib": ">=2.0.0 <3.0.0",
    "typescript": ">=4.7.0 <6.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
